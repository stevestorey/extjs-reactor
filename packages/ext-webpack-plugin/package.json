{
  "name": "@extjs/ext-webpack-plugin",
  "version": "0.0.42",
  "description": "A webpack plugin for new Ext JS applications",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib"
  },
  "dependencies": {
    "@extjs/ext-build": "~0.0.1",
    "lodash.uniq": "^4.5.0",
    "schema-utils": "^0.4.5",
    "chalk": "^2.4.1",
    "recursive-readdir-sync":"^1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env":"^1.7.0"
   },
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "stage-0"
    ],
    "sourceMaps": "inline",
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sencha/extjs-reactor"
  },
  "keywords": ["webpack","webpack-plugin","EXT JS", "Sencha"],
  "author": "Sencha, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sencha"
  },
  "homepage": "https://github.com/sencha/extjs-reactor/tree/master/packages/ext-webpack-plugin"
}
